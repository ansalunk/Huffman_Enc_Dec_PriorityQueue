package com.datastructure;

public class PairNode {

		public int element;
		public PairNode leftChild;
	    public PairNode nextSibling;
	    public PairNode prev;
	    public int freq;
	 
	    /* Constructor */
	    public PairNode(int node, int freq)
	    {
	    	this.freq = freq;
	        element = node;
	        leftChild = null;
	        nextSibling = null;
	        prev = null;
	    }

		public PairNode() {
			// TODO Auto-generated constructor stub
			super();
		}
}
