package com.datastructure;

public class PairNode {

		int element;
	    PairNode leftChild;
	    PairNode nextSibling;
	    PairNode prev;
	    int freq;
	 
	    /* Constructor */
	    public PairNode(int node, int freq)
	    {
	    	this.freq = freq
	        element = node;
	        leftChild = null;
	        nextSibling = null;
	        prev = null;
	    }

		public PairNode() {
			// TODO Auto-generated constructor stub
			super();
		}
}
